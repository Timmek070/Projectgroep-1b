            <Font name="Corbel" size="15.0" />
         </font></Button>
      <Button fx:id="shopButton" layoutX="224.0" layoutY="131.0" mnemonicParsing="false" onMouseClicked="#shop" prefHeight="106.0" prefWidth="126.0" text="Shop">
