<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ShopController">
   <children>
      <Label layoutX="23.0" layoutY="42.0" text="Welcome to the Kumbaya ZeroXess Shop" textFill="#004a80">
         <font>
            <Font name="Cooper Black" size="27.0" />
         </font></Label>
      <Button fx:id="backButton" layoutX="421.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#backToHome" prefHeight="42.0" prefWidth="141.0" text="Back to homepage">
         <font>
            <Font name="Corbel" size="15.0" />
         </font>
      </Button>
      <Button fx:id="Shopping" layoutX="40.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#toShop" prefHeight="42.0" prefWidth="141.0" text="Shopping Cart">
         <font>
            <Font name="Corbel" size="15.0" />
         </font>
      </Button>
      <Text layoutX="82.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="&quot;Closing Africa's connectivity gap to power social and economic progress&quot; ">
         <font>
            <Font name="Corbel Light" size="14.0" />
         </font>
      </Text>
      <MenuButton layoutX="462.0" layoutY="114.0" mnemonicParsing="false" text="Categories">
        <items>
          <MenuItem mnemonicParsing="false" text="gaming" />
          <MenuItem mnemonicParsing="false" text="nog meer" />
