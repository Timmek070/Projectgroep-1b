<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ShoppingCartController">
    <children>
        <Label layoutX="60.0" layoutY="42.0" text="Welcome to the Kumbaya ZeroXess" textFill="#004a80">
            <font>
                <Font name="Cooper Black" size="27.0" />
            </font>
        </Label>
        <Label layoutX="200.0" layoutY="75.0" text="Shopping Cart" textFill="#004a80">
            <font>
                <Font name="Cooper Black" size="27.0" />
            </font>
        </Label>
        <Button fx:id="closeButton" layoutX="20.0" layoutY="337.0" mnemonicParsing="false" onMouseClicked="#closeShop" prefHeight="42.0" prefWidth="141.0" text="Back to shop">
            <font>
                <Font name="Corbel" size="15.0" />
            </font>
        </Button>

        <Button fx:id="printReceipt" layoutX="197.0" layoutY="337.0" mnemonicParsing="false" onMouseClicked="#printReceipt" prefHeight="42.0" prefWidth="141.0" text="Print Receipt">
            <font>
                <Font name="Corbel" size="15.0" />
            </font>
        </Button>
        <Text layoutX="100.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="&quot;Closing Africa's connectivity gap to power social and economic progress&quot; ">
            <font>
                <Font name="Corbel Light" size="14.0" />
            </font>
        </Text>
        <Text fx:id="gaming" layoutX="100.0" layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font name="Corbel Light" size="14.0" />
            </font>
        </Text>
        <TableView fx:id="table" layoutX="90.0" layoutY="147.0" prefHeight="178.0" prefWidth="431.0">
            <columns>
                <TableColumn fx:id="name" minWidth="0.0" prefWidth="115.0" text="Naam" />
                <TableColumn fx:id="price" prefWidth="127.0" text="Prijs" />
                <TableColumn fx:id="category" minWidth="8.0" prefWidth="106.0" text="Categorie" />
                <TableColumn fx:id="button" prefWidth="82.0" resizable="false" />
            </columns>
        </TableView>
        <TextArea fx:id="bruh" layoutX="200.0" layoutY="400.0" prefHeight="200.0" prefWidth="200.0" />
    </children>
</AnchorPane>
