<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisterController">
    <children>
        <Label layoutX="60.0" layoutY="42.0" text="Welcome to the Kumbaya ZeroXess" textFill="#004a80">
            <font>
                <Font name="Cooper Black" size="27.0" />
            </font>
        </Label>
        <Button fx:id="backButton" layoutX="37.0" layoutY="340.0" mnemonicParsing="false" onMouseClicked="#backToWelcome" prefHeight="42.0" prefWidth="141.0" text="Back to Welcome Page">
            <font>
                <Font name="Corbel" size="12.0" />
            </font>
        </Button>
        <TextField fx:id="emailField" layoutX="37.0" layoutY="150.0" />
        <Text layoutX="37.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email" />
        <TextField fx:id="usernameField" layoutX="37.0" layoutY="202.0" />
        <Text layoutX="37.0" layoutY="200.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
        <PasswordField fx:id="passwordField" layoutX="37.0" layoutY="250.0" />
        <Text layoutX="37.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
        <PasswordField fx:id="confPasswordField" layoutX="37.0" layoutY="300.0" />
        <Text layoutX="37.0" layoutY="300.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Confirm Password" />

        <TextField fx:id="nameField" layoutX="400.0" layoutY="150.0" />
        <Text layoutX="400.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
        <TextField fx:id="sexField" layoutX="400.0" layoutY="200.0" />
        <Text layoutX="400.0" layoutY="200.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sex" />
        <TextField fx:id="birthdateField" layoutX="400.0" layoutY="250.0" />
        <Text layoutX="400.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Birthdate" />
        <TextField fx:id="roleField" layoutX="400.0" layoutY="300.0" />
        <Text layoutX="400.0" layoutY="300.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Role" />
        <Button fx:id="registerButton" layoutX="400.0" layoutY="340.0" mnemonicParsing="false" onMouseClicked="#CreateUser" prefHeight="42.0" prefWidth="141.0" text="Register">
            <font>
                <Font name="Corbel" size="12.0" />
            </font>
        </Button>
        <Text fx:id="foutmeldingText" fill="#ff0202" layoutX="205.0" layoutY="340.0" strokeType="OUTSIDE" strokeWidth="0.0" text="" />
    </children>

</AnchorPane>
